<div class="box">
    <div class="box-header with-border">
      <h3 class="box-title">Expiring Domains</h3>
    </div>
    <!-- /.box-header -->
    <div class="box-body">
      <div class="dataTables_wrapper dt-bootstrap" id="example_wrapper"></div>
      <table class="table table-bordered" id="domains" data-source="<%= domain_domain_index_path(format: "json") %>">
      <thead>
      <tr>
        <th>ID</th>
        <th>Domain Name</th>
        <th>Domain Price</th>
        <th>Number Of Bids</th>
        <th>Domain Valuation</th>
        
        <th>Traffic</th>
        <th>is Adult</th>
        <th>Auction End Time</th>
        <th>Has Website</th>
        <th>Source</th>
        <th>Filter Matched</th>
      </tr>
      </thead>
      <tbody>

    </tbody>
      </table>
      
      <div class="box-footer clearfix">
        Filter Has Websites :
            <select id="haswebsite">
              <option value=''>Has Websites</option>
              <option value="1">True</option>
              <option value="0">False</option>
            </select>
            
            Filter Feed Source :
            <select id="source">
              <option value=''>Feed Source</option>
              <option value="GoDaddy Auctions">Go Daddy Auctions</option>
              <option value="SnapNames">SnapNames</option>
            </select>
            
            Filter Domain Ending :
            <select id="domainending">
              <option value=''>Domain Ending</option>
              <option value=".COM">.com</option>
              <option value=".CO.UK">.co.uk</option>
               <option value=".INFO">.info</option>
              <option value=".UK">.uk</option>
              <option value=".NET">.net</option>
              <option value=".ORG">.org</option>
            </select>
            
             Filter Domain Ending :
            <select id="endtime">
              <option value=''>Domain Expire Date</option>
              <option value="today">Today</option>
              <option value="tomorrow">Tomorrow</option>
               <option value="2days">2 days</option>
              <option value="3days">3 Days</option>
              <option value="4days">4 Days</option>
              <option value="5days">5 Days</option>
            </select>
            
            
            </div>
            
            
          </div>
          </div>
